<div class="boxContainer">
<div class="page-header">
        <div class="headbox01">入院 病棟<br>緩和ケア</div>
        <div class="headbox02">
          <p class="sample1">ID : <%= @patient.id %><br>
        ②後期　本人（30%）</p>
        </div>
        
        <div class="headbox03">
          <% if @patient.gender == '男' %>
          <%= image_tag 'men.png', :title => '男性', :size => '100x50', :style=>"float:left;" %><br>
          <% else %>
          <%= image_tag 'women.png', :title => '女性', :size => '100x50', :style=>"float:left;" %><br>
          <% end %>
   
<!--        <%= @patient.kana %><br>  -->
          <font size="5"><u>
            <ruby>
            <rb><%= @patient.pname %></rb>
            <rp>（</rp><rt><%= @patient.kana %></rt><rp>）</rp>
            </ruby>
          </u></font></div>
        <div class="headbox05">
          <% if @patient.infection == '有' %>
          <%= image_tag 'alrt_y.png', :title => '感染症：HCV(+)', :size => '40x40', :style=>"float:left;" %><br>
          <% else %>
          <%= image_tag 'alrt_n.png', :title => '感染症未確認', :size => '40x40', :style=>"float:left;" %><br>
          <% end %>
        </div>
        <div class="headbox05">
          <% if @patient.id == 3 %>
          <%= image_tag 'arg_y.png', :title => '青魚禁', :size => '40x40', :style=>"float:left;" %><br>
          <% else %>
          <%= image_tag 'arg_n.png', :size => '40x40', :style=>"float:left;" %><br>
          <% end %>
        </div>
        <div class="headbox04">
          <%= @patient.age %>歳00ヶ月&ensp;&ensp;&ensp;<%= @patient.bloodtype %>型Rh(+)&ensp;&ensp;&ensp;<u><%= @patient.height %>cm&ensp;<%= @patient.weight %>Kg</u></div>
        <div class="headbox05">
          <p class="sample1">
<a href="/profiles/<%= @patient.id %>" class="btn btn-default btn-sm active" role="button">Prof</a><br>
<a href="/profiles/<%= @patient.id %>" class="btn btn-default btn-sm active" role="button" disabled="disabled">病名</a>
          </p></div>
