<div class="row">
  <div class="col-md-6 col-md-offset-3">

<%= form_for(prescription) do |f| %>
  <%= render 'layouts/error_messages', model: f.object %>

<div class="field">
  <span class="job_type_engine">実施の有無</span><br>
  <div class="radio-inline">
    <%= f.radio_button :po01, :し無０１, checked: "checked" %><%= f.label :無, class: "po01_label" %>
  </div>
  <div class="radio-inline">
    <%= f.radio_button :po01, :し有０１ %><%= f.label :有, class: "po01_label" %>
  </div>
  <div class="radio-inline">
