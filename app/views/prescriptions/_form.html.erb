  </div>
</div>
<div class="pty01_field">
  <%= f.label :pty01, '予定年月日' %>
  <%= f.datetime_select(:pty01, start_year: 2018, end_year: (Time.now.year - 19), default: Date.new(2018, 1,15)) %>
</div>
<div class="ptj01_field">
  <%= f.label :ptj01, '実施年月日' %>
