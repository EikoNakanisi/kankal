<header>
  <nav class="navbar navbar-default navbar-fixed-top">
  <div class="container">
      <div class="navbar-header">
        <p><font size="+1" color="#000000">テンプレート参照</font><BR>
        ID : <%= @template.id %></p>
      </div>
      <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
        <ul class="nav navbar-nav navbar-right">


  <% if @template.id > 20 %>
    <a href="../patients/<%= @template.id-@hensu2 %>#a8"><img src="../images/close_btn.png" border="0"></a>
